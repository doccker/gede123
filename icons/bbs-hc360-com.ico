function inputSeachQestion(){
	var oldKey = $("#search_keywords_val").val()
	var key = $('.search_input').val();
	var wendaSeachHtml = $(".wenda_search_list").html();
	if(key.length > 0 ){
		if(key != oldKey){
			$("#search_keywords_val").val(key);
			$.ajax({
		        url:"http://sxy.hc360.com/index.php?app=public&mod=Search&act=getQuestionSearchForAjaxNeed",
		        data:{key:key},
		        type:'GET',
		        dataType:'JSON',
		        async:true,
		        error:function(e){
		        	ui.error("请求错误");
		        },
		        success:function(json){
		        	if(json.status == 1){
		        		$(".wenda_search").html(json.data);
		        		$(".wenda_search").show();
		        	}else if(json.status == 0){
		        		$(".wenda_search").hide();
		        	}
		        }
		    });
		}else if(wendaSeachHtml != ""){
			$(".wenda_search").show();
		}
		
	}else{
		$(".wenda_search").hide();
	}
}
function wendaTableHide(){
	$(".wenda_search").hide();
}
function clear_news(b){
	var thisNumber =  $("."+b).find(".news_num").text();
	var allNumber = $("#all_news_num").text();
	if(allNumber != ""){
		allNumber = allNumber - thisNumber;
		if(allNumber == 0){
			$("#all_news_num").remove();
		}else{
			$("#all_news_num").text(allNumber);
		}
		
	}
	$("."+b).find(".news_num").remove();
}

function keydown(evt){
	var evt=evt?evt:(window.event?window.event:null);//兼容IE和FF
	if (evt.keyCode==13){
		ajaxssologins();
	}
}
get_regvalidkey();
function get_regvalidkey(){
	$.getJSON("http://sso.hc360.com/LoginTicket.jsp?&callback=?",
	function(json) {
		if(json!=null){
			$('#popLoginTicket').val(json.key);
		}
	});
} 

function get_regvalidkeys(){
	$("#log").html('<span>验证码</span><input onkeydown="keydown(event)" type="text" size="5" id="ulogcode" value="" style="width:60px;margin-right:10px;"/><a href="javascript:get_regvalidkeys();"><img id="logcode_validimg" src="" />看不清？换一换</a>');
	$("#log").slideDown('slow');
	$.getJSON("http://sso.hc360.com/LoginTicket.jsp?&callback=?",
	function(json) {
		if(json!=null){
			$('#popLoginTicket').val(json.key);
			_newUrl='http://sso.hc360.com/ValidImage.jsp?seed='+json.value;
			$('#logcode_validimg').attr('src',_newUrl);
		}
	});
} 

function ajaxssologins() {
	  returnurl = document.URL;
	  var ulogcodes = '';
	  if( $("#ulogcode").val() ){ ulogcodes = $("#ulogcode").val(); }
      $.getJSON("http://sso.hc360.com/ssologin?callback=?",
      {
        LoginType : "json",
        LoginID : document.getElementById("snsname").value,
        Passwd : document.getElementById("snspass").value,
        ValidKey : ulogcodes,
        ctoken : document.getElementById("popLoginTicket").value,
        ReturnURL : returnurl
      },
      function(json) {
	        if ( json != null) {
				if( json.success == "yes" ){
					$("#loginmsg").toggle();
					$("#loginmsg").html("登录成功...");
					var fun = $("#clickfun").val();
					if(fun=='0'){
						window.location.reload();
					}else{
						//隐藏登录框
						$(".divbg").hide();
						$(".msg_login").hide();
						$("#snspass").val('');
						$("#loginmsg").html("");
						//ajax去后台判断登录  并写session
						$.post("http://sxy.hc360.com/index.php?app=public&mod=Passport&act=quickLoginAjax",function(msg){
							if(msg>0){
								//MID = //设置js登录id
								MID = msg;
								$("#clickfun").val('0')
								//执行传过来的fun
								eval(fun);
								$.post("http://sxy.hc360.com/index.php?app=widget&mod=Login&act=updateDate",function(data){
									if(data != '0'){
										$("#header_tools").html(data);
									}else{
										window.location.reload();
									}
								});
							}else{
								$("#loginmsg").toggle();
								if(msg==0){
									alert('登录失败！');	
								}else{
									alert('该用户已被禁用！');
								}
								
							}
						});
					}
					
				}else{
					$(".snspass").val('');
					get_regvalidkeys();
					$("#loginmsg").toggle();
					$("#loginmsg").html(json.value);
				}
	        } else {
				$(".snspass").val(''); 
				get_regvalidkeys();
	          
	        }
      });
}
function rmNoctie(type){
	$.post("http://sxy.hc360.com/index.php?app=public&mod=Feed&act=rmUserNotice", { 'type': type } );
}
