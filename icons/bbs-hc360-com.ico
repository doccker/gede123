//检查发布问题的标题
function checktitle(){
	//标题长度验证
	if($("#title").val().replace(/(^\s*)|(\s*$)/g, "").length <6){
		$("#title_tip").addClass("not_ok");
		$("#title").addClass("errorInput");
		return false;
	}else{
		$("#title_tip").removeClass("not_ok");
		$("#title").removeClass("errorInput");
		return true;
	}
}

//发布问题
function subQuestion(){
	if(!checktitle()){
		return false;
	}
	var award_nums=parseInt($(".wenda_award_select").val());
    var wealth_nums=parseInt($(".wenda_award_nums i").text());
    if(award_nums>wealth_nums){
        $(".wenda_award_tip").show();
        return false;
    }else{
        $(".wenda_award_tip").hide();
    }
    var title  = $("#title").val();
    var cate   = $("#questionCate").val();
    var source = $("#source").val();
    var sourceid = $("#sourceid").val();
    var content = ue.getContent();
    if(ue.getContentLength(true)>1000){
    	return false;
    }
    $.ajax({
        url:"http://sxy.hc360.com/index.php?app=public&mod=Question&act=addQuestion",
        data:{cate:cate,title:title,content:content,awardNums:award_nums,source:source,sourceid:sourceid},
        type:'POST',
        dataType:'JSON',
        async:true,
        error:function(){
        	ui.error('请求错误');
        },
        success:function(json){
        	if(json.status != 4){
        		ui.error(json.data);
        	}else{
        		$("#newid").val(json.newid);
        		//隐藏发布窗口
                $(".answer_msg").hide();
        		//打开提交成功邀请窗口
        		var document_height=$(document).height();
        	    var window_height=$(window).height();
        		var scroll_height=$(document).scrollTop();
                $(".divbg").css("height", document_height);
                $(".divbg").show();
                $(".wenda_msg").css("top",scroll_height+window_height/2-265);
                $(".wenda_msg").show();
                $(".classify_item").eq(0).addClass("item_selected").siblings(".classify_item").removeClass("item_selected");
        		$(".hd_tip").eq($('.classify_item').index($(".classify_item").eq(0))).show().siblings(".hd_tip").hide();
        		$("#questionCate").val($($(".classify_item").eq(0)).attr("data"));
        		$("#title").val("");
        		ue.setContent("");
        	}
        }
    });
    return false;
}
</script>

