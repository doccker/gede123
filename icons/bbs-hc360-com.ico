	        url:"http://sxy.hc360.com/index.php?app=widget&mod=Report&act=submitReportInfo",
	        data:{type:type,app_id:app_id,reportVal:reportVal,reportOtherContent:reportOtherContent},
	        type:'POST',
	        dataType:'JSON',
	        error:function(){
	        	ui.error('请求错误');
	        },
	        success:function(json){
	        	if(json.status == 1){
	        		ui.success(json.data);
	        		$(".report_"+type+"_"+app_id).removeClass("report");
	        		$(".report_"+type+"_"+app_id).addClass("report2");
	        		$(".report_"+type+"_"+app_id).removeAttr("onclick");
	        		$(".report_"+type+"_"+app_id).removeAttr("href");
	        		$(".report_"+type+"_"+app_id).html("已举报");
	        		report.closeReportWindow();
	        	}else if(json.status == 0){
	        		ui.error(json.data);
	        		return false;
	        	}else if(json.status == 2){
	        		$(".report_"+type+"_"+app_id).removeClass("report");
	        		$(".report_"+type+"_"+app_id).addClass("report2");
	        		$(".report_"+type+"_"+app_id).attr("onclick","");
	        		$(".report_"+type+"_"+app_id).html("已举报");
	        		report.closeReportWindow();
	        		ui.error(json.data);
	        		return false;
	        	}
	        }
	    })
	 return false;
	 
 }
