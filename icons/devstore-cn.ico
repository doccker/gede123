<script  type="text/javascript">
$(function($) {
	$(".sreach_sort_list li").click(function(){
		$("#_pName").val($(this).attr("l"));
	});
	$(".button_sreach").click(function(){
		if($(".text_sreach").val() !="")
		{
			var key ;
			var opt =  $("#top_keyword").val().indexOf(".");
			if(opt != -1){
				key = $("#top_keyword").val().slice(0,opt)+","+$("#top_keyword").val().slice(opt+1);
			}else{
				key = $("#top_keyword").val();
			}
			key=key.toString().replace(/-/g,"%#");
			window.location="/search/"+$("#_pName").val()+"/"+encodeURIComponent(key);	
		}else
		{
			alert("请填写关键词后搜索");
		}
		
	});
	$(".text_sreach").keydown(function(event){
		  if(event.keyCode =="13") {
			$(".button_sreach").click();
		  }
	});
	
	/*搜索效果*/  
	$(".search_input input").focus(function(){
		if(  $(this).width() == 150 ){
		$(this).animate({width:"250px"});
		}else{
		$(this).animate({width:"120px"});
		}
	});
	$(".search_input input").focusout(function(){
		if( $(this).val() == ""){
			if(  $(this).width() == 250 ){
			$(this).animate({width:"150px"});
			}else{
			$(this).animate({width:"100px"});
			}
		}
	});
	function search(searchStr){
		var result=[];
		$.ajax({
			type: "POST",
			dataType: "json",
			url: '/public_data/getSearchKey',
			async: false,
			data: {searchStr:searchStr},
			success: function(data) {
				if(data.status==1){
					var list=data.list;
					for(var i=0;i<list.length;i++){
						result.push(list[i].name);
					}
				}
			}
		});
		return result;
	}
	
	$(".search_input input").autocomplete({
		minLength: 1,
		source:function(request, response){
			var result=search(request.term);
			$('.ui-autocomplete').css("z-index",9999);
			response(result);
		},
		delay:600
	});
	
    switch($("#titleInput").val()){
    case "product":$(".sreach_sort_list li:eq(0)").click();break;
    case "news":$(".sreach_sort_list li:eq(1)").click();break;
    case "eassy":$(".sreach_sort_list li:eq(2)").click();break;
    case "question":$(".sreach_sort_list li:eq(3)").click();break;
    case "project":$(".sreach_sort_list li:eq(4)").click();break;
	case "resource":$(".sreach_sort_list li:eq(5)").click();break;
	case "sourceCode":$(".sreach_sort_list li:eq(6)").click();break;
	case "evaluation":$(".sreach_sort_list li:eq(7)").click();break;
	case "company":$(".sreach_sort_list li:eq(8)").click();break;
	case "data":$(".sreach_sort_list li:eq(9)").click();break;
	case "user":$(".sreach_sort_list li:eq(10)").click();break;
	}	
	
});
</script>		<!--导航-->
