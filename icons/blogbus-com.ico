						$("#more").remove();
						$("#infscr-loading").hide();
						$("#pages").show();
						$(window).unbind('.infscr');
					}
				}	
			},errorCallback:function(){ 
				$("#pages").show();
			}
			
		},function(newElements){
			var $newElems = $(newElements);
			$('.stream').masonry('appended', $newElems, false);
			$newElems.fadeIn();
			item_masonry();
			return;
		});
	});
