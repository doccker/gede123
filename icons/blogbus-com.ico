<script>
	function item_masonry()
	{
		var $container = $('.stream');
		$container.imagesLoaded( function(){
		  $container.masonry({
			itemSelector: '.item'
		  });
		});
	}
	$(function(){
		var perpage = 10;
		item_masonry();
		
		var sp = 1;
		$(".stream").infinitescroll({
			navSelector  	: "#more",
			nextSelector 	: "#more a",
			itemSelector 	: ".item",
			debug        : true,
			extraScrollPx : 50,
			animate: false,
			loading:{
				img: "http://public2.blogbus.com/bbc-v6/front/img/masonry_loading_1.gif",
				msgText: ' ',
				finishedMsg: '',
				finished: function(){
					sp++;
