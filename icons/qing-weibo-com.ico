function position(ele){
	var x = 0, y = 0, b = document.body, d = document.documentElement;
	while(ele.offsetParent){
		x = x + ele.offsetLeft;
		y = y + ele.offsetTop;
		ele = ele.offsetParent;
		if(ele !== b && ele !== d && ele !== document){
			x = x - ele.scrollLeft;
			y = y - ele.scrollTop;
		}
	}
	return {
		left : x,
		top : y
	}
}

function fixNum(n){
	n = n - 0;
	return n < 10 ? '0' + n : '' + n;
}

function fixTime(s){
	var t = new Date(s * 1000);
	return (t.getMonth() + 1) + '月' + (t.getDate()) + '日 ' + fixNum(t.getHours()) + ':' + fixNum(t.getMinutes());
}

function contains(p,c){
	if(p === c){return false;}
	if(p.compareDocumentPosition){return p.compareDocumentPosition(c) === 20;}
	if(p.contains){return p.contains(c);}
}

function clipWords (str, a, b) {
    var str = str.replace(/(^[\s|&nbsp;]*)|($[\s|&nbsp;]*)/g, '');
    var s = str.replace(/([^\x00-\xff])/g, "\x00$1");
    var ret = (s.length < a) ? str : (s.substring(0, a - 3).replace(/\x00/g, '') + '...');
    var ret1, ret2;
    if(b && b < a){
        ret1 = s.substring(0, b).replace(/\x00/g, '');
        ret2 = s.substring(b).replace(/\x00/g, '').match(/[^。！？；]+[。！？；]/g);
        if(ret2){
            ret = ret1 + ret2[0];
        }
    }
    return ret;
}

function getMouseDocPos(e){
