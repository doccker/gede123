		tli = '<li><a page="%page%" href="%url%" target="_blank" class="%ispad%">%stitle%<i class="slide_guess_del" durl="%durl%">&emsp;</i></a></li>',
		api = 'http://cre.mix.sina.com.cn/api/v3/get?type=8&length=%length%&cateid=%cateid%&fields=%fields%&oe=gbk&dedup=2&callback=guessCallback&cre=blogpc&mod=g&r=u&loc=2&merge=3&statics=1',
		api2 = 'http://cre.mix.sina.com.cn/cross/off/url',
		cateid = '6GT',
		fields = ['url','title'],
		page = 5,
		num = 11,
		sp = null,
		gdata = null,
		timerTip = null,
		dc = 0,
