			return info.uid;
		},
		handdleFailure = function(){
			paint();
		},
		showTip = function(){
			wtip.style.display = 'block';
			timerTip && clearTimeout(timerTip);
			timerTip = setTimeout(function(){
				wtip.style.display = 'none';
			},2000);
		},
		paint = function(s,p,b){
			if(!b){
				if(s){
					w.setAttribute('suda-uatrack','key=blog_index_guess&value=recommend_click');
					try{SUDA._S_uaTrack('blog_index_guess','recommend_show');}
					catch(e){}
				}else{
					w.setAttribute('suda-uatrack','key=blog_index_guess&value=hot_click');
					try{SUDA._S_uaTrack('blog_index_guess','hot_show');}
					catch(e){}
				}
			}
			if(getUid()){addClass(w,'slide_guess_logined');}
			else{removeClass(w,'slide_guess_logined');}
			var datas = s || window.BLOGGUESSDATA || [],
				h = [],
				l = Math.min(datas.length,page * num),
				ispad = window.isPad ? 'ispad' : '',
				title,url,
				i,d,pg;
			for(i = 0; i < l; i++){
				d = datas[i];
				if(i % num === 0){
					h.push('<div class="slide_guess_items fl">');
					h.push('<ul>');
				}
				pg = Math.floor(i / num);
				title = d['title'] || '';
				stitle = title.slice(0,16);
				url = d['url'];
				h.push(tli.replace(/%page%/g,pg).replace(/%url%/g,url).replace(/%durl%/g,url).replace(/%title%/g,title).replace(/%stitle%/g,stitle).replace(/%ispad%/g,ispad));
				if(i % num === num - 1 || i === l - 1){
					h.push('</div>');
					h.push('</ul>');
				}
			}
			w.innerHTML = h.join('');
			sp = new ScrollPic();
			sp.scrollContId = wid;
			sp.pageWidth = 300;
			sp.frameWidth = 300;
			sp.speed = 10;
			sp.pageIndex = p || 0;
			sp.autoPlay = false;
			sp.onpagechange = function(){
				windex.innerHTML = sp.pageIndex + 1 + ' / ' + sp.pageLength;
			};
			sp.initialize();
			windex.innerHTML = sp.pageIndex + 1 + ' / ' + sp.pageLength;
		};
	addEvent(wprev,'click',function(){
		if(sp){sp.pre();}
	});
	addEvent(wnext,'click',function(){
		if(sp){sp.next();}
	});
	if(!window.isPad){
		addEvent(w,'mousemove',function(ev){
			var e = ev || window.event,
				t = e.target || e.srcElement,
				p;
			if(!t.nodeName){t = t.parentNode;}
			if(hasClass(t,'slide_guess_del')){
				p = position(t);
				wdtip.style.display = 'block';
				wdtip.style.top = (p.top - 40) + 'px';
				wdtip.style.left = (p.left - 35) + 'px';
			}else{
				wdtip.style.display = 'none';
			}
		});
		addEvent(w,'mouseout',function(ev){
			var e = ev || window.event,
				t = e.target || e.srcElement;
			wdtip.style.display = 'none';
		});
	}
	delegate(w,'slide_guess_del','click',function(e,t){
		if(e.preventDefault){
			e.preventDefault();
			e.stopPropagation();
		}else{
			e.returnValue = false;
			e.cancelBubble = true;
		}
		try{SUDA._S_uaTrack('blog_index_guess','recommend_del');}
		catch(e){}
		var durl = t.getAttribute('durl'),
			uid = getUid(),
			n = (sp || {}).pageIndex || 0;
		gdata = (function(s){
			if(!s){return null;}
			var l = s.length,
				i;
			for(i = 0; i < l; i++){
				if(s[i]['url'] === durl){
					s.splice(i,1);
					dc++;
					return s;
				}
			}
			return s;
		})(gdata);
		if(uid){
			new Ijax({
				url : api2,
				method : 'post',
				data : {
					uid : uid,
					url : durl
				}
			});
		}
		if(dc % num === 0){
			setTimeout(function(){
				GUESSSLIDE.refresh();
				try{SUDA._S_uaTrack('blog_index_guess','recommend_delpage');}
				catch(e){}
			},400);
			showTip();
		}else{
			paint(gdata,(sp || {}).pageIndex || 0,true);
		}
	});
	window.guessCallback = function(s){
		var datas;
		if(!s){
			handdleFailure();
			return;
		}
		datas = (function(d,t){
			var ti,tl,td;
			d = d || [];
			t = t || [];
			for(ti = 0, tl = t.length; ti < tl; ti++){
				td = t[ti];
				td['r'] = 'hot';
				d.push(td);
			}
			return d;
		})(s.data,s.top);
		if(!datas.length){
			handdleFailure();
			return;
		}
		gdata = datas;
	};
	window.GUESSSLIDE = {
		refresh : function(){
			gdata = null;
			dc = 0;
			getScript({
				url : api.replace(/%length%/,(page + 1) * num).replace(/%cateid%/,cateid).replace(/%fields%/,fields.join(',')) + '&_=' + new Date().getTime(),
				charset : 'utf-8',
				timeout : 10e3,
				ontimeout : function(){
					handdleFailure();
				},
				onerror : function(){
					handdleFailure();
				},
				oncomplete : function(){
					paint(gdata);
				}
			});
		}
	};
})();

