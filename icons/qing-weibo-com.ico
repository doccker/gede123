						wusername = getByClass('blog_user_uname',that.wyes)[0],
						img = infos[4][1],
						profile = isblog ? that.tnotice : that.tregister,
						ws,l;
					that.isblog = isblog;
					that.wyes.innerHTML = that.tlogined.replace(/%profile%/g,profile).replace(/%uid%/g,uid).replace(/%uname%/g,uname).replace(/%suname%/g,suname).replace(/%img%/g,img).replace(/%ncmnt%/g,ncmnt).replace(/%nnotice%/g,nnotice).replace(/%nfeed%/g,nfeed);

					ws = that.wyes.getElementsByTagName('span');
					l = ws.length;

					if(isblog){
						// that.paintCatenav();
						getByClass('blog_user_logined_data',that.wyes,'div')[0].style.display = 'block';
						getByClass('blog_user_logined_action',that.wyes,'div')[0].style.display = 'block';
						that.wBlogrecom.style.display = 'none';
						getScript({
							url : that.apiNotice.replace(/%uid%/,uid),
							oncomplete : function(){
								if(typeof BLOGNOTICE === 'undefined' || !BLOGNOTICE['data'] || !BLOGNOTICE['data'][uid]){return;}
								var d = BLOGNOTICE['data'][uid],
									i,e,a;
								for(i = 0; i < l; i++){
									e = ws[i];
									a = e.getAttribute('msgtype');
									if(a === 'cmnt'){e.innerHTML = d.blogcomment || 0;}
									if(a === 'notice'){e.innerHTML = d.unreadnotices || 0;}
									if(a === 'feed'){e.innerHTML = d.feed || 0;}
								}
							}
						});
						getScript({
							url : that.apiBloginfo.replace(/%uid%/g,uid),
							oncomplete : function(){
								if(typeof requestId_69738672 === 'undefined' || !requestId_69738672['data']){return;}
