<script>

function addEvent(ele,etype,fn){
	ele = ele || window.event || {};
	if(ele.addEventListener){
		ele.addEventListener(etype,fn,false);
	}else if(ele.attachEvent){
		ele.attachEvent('on' + etype,fn);
	}else{
		ele['on' + etype] = fn;
	}
};

function removeEvent(ele,etype,fn){
	if(ele.removeEventListener){
		ele.removeEventListener(etype,fn,false);
	}else if(ele.detachEvent){
		ele.detachEvent('on' + etype,fn);
	}else{
		ele['on' + etype] = null;
	}
};
function delegate(ele,cname,etype,fn){
	addEvent(ele,etype,function(e){
		var e = e || window.event,
			t = e.target || e.srcElement;
		if(!t.nodeName){t = t.parentNode;}
		do{
			if(hasClass(t,cname)){
				fn(e,t);
				return;
			}
			t = t.parentNode;
		}while(t != ele && t.parentNode)
	});
};

function hasClass(ele,cname){
	var reg = new RegExp('(^| )' + cname + '( |$)'),
		ename = ele.className;
	return reg.test(ename);
}

function addClass(ele,cname){
	if(!hasClass(ele,cname)){
		ele.className += ' ' + cname;
	}
};

function removeClass(ele,cname){
	var re = new RegExp('(^| )' + cname + '( |$)');
	if(cname){
		ele.className = ele.className.replace(re,' ').replace(/^\s+|\s+$/g,'');
	}else{
		ele.className = '';
	}
};

function getByClass(searchClass,node,tag){
	if(node == null){node = document;}
	if(tag == null){tag = '*';}
	var classElements = [],
		els = node.getElementsByTagName(tag),
		elsLen = els.length;
		for(i = 0; i < elsLen; i++){
			if(hasClass(els[i],searchClass)){
				classElements.push(els[i]);
			}
		}
	return classElements;
};

function getByAttr(aname,avalue,node,tag){
	if(!node){node = document;}
	if(!tag){tag = '*';}
	var ret = [],
		eles = node.getElementsByTagName(tag),
		l = eles.length;
	for(var i = 0; i < l; i++){
		var d = eles[i],
			a = d.getAttribute(aname);
		if(avalue !== null && a !== avalue || a === null){continue;}
		ret.push(d);
	}
	return ret;
}

function getScript(cfg){
	if(!cfg || !cfg.url){return;}
	var url = cfg.url,
