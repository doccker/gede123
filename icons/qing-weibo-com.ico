		else{islogin = false;}
		if(islogin === logined){return;}
		logined = islogin;
		// 去掉对原feed流的调用 20150610 delete by wjw
		if(s){bloglogin.gin(s);}
		else{bloglogin.gout();}
		// BLOGLOGIN.fillFeeds();
		GUESSSLIDE.refresh();
		//HotFollowed.refresh();
	};
	setInterval(checkLogin,200);
})();
