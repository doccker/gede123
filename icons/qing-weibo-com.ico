			var $level;
			if(!$pv){return ''};
			if($pv < 50){$level = 0;}
			else if($pv < 100){$level = 1;}
			else if($pv < 150){$level = 2;}
			else if($pv < 200){$level = 3;}
			else if($pv < 300){$level = 4;}
			else if($pv < 500){$level = 5;}
			else if($pv < 800){$level = 6;}
			else if($pv < 1500){$level = 7;}
			else if($pv < 3000){$level = 8;}
			else if($pv < 5000){$level = 9;}
			else if($pv < 10000){$level = 10;}
			else if($pv < 15000){$level = 11;}
			else if($pv < 25000){$level = 12;}
			else if($pv < 40000){$level = 13;}
			else if($pv < 70000){$level = 14;}
			else if($pv < 100000){$level = 15;}
			else if($pv < 150000){$level = 16;}
			else if($pv < 200000){$level = 17;}
			else if($pv < 500000){$level = 18;}
			else if($pv < 1000000){$level = 19;}
			else if($pv < 2000000){$level = 20;}
			else if($pv < 5000000){$level = 21;}
			else if($pv < 10000000){$level = 22;}
			else if($pv < 20000000){$level = 23;}
			else if($pv < 50000000){$level = 24;}
			else{$level = Math.floor($pv/50000000) + 24;}
			return $level;
		}
	};
